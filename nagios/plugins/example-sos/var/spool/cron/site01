# --ENVIRONMENT------------------------------------------------
OMD_ROOT=/omd/sites/site01
OMD_SITE=site01
PATH=/omd/sites/site01/local/bin:/omd/sites/site01/bin:/usr/local/bin:/bin:/usr/bin:/sbin:/usr/sbin
# ------------------------------------------------------------
# /omd/sites/site01/etc/cron.d/logrotate
#
# Daily Logrotate
#
0 0 * * * /usr/sbin/logrotate -s $OMD_ROOT/tmp/run/logrotate.state $OMD_ROOT/etc/logrotate.conf >/dev/null 2>&1
